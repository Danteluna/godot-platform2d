[sub_resource type="GDScript" id=10]

script/source = "extends StaticBody2D

func _ready():
	create_shapes()

func create_shapes():
	while !get_shape_owners().empty():
		remove_shape_owner(0)
	for c in get_children():
		if c.is_class(\"Polygon2D\"):
			print(\"found a polygon\")
			var shape_owner = create_shape_owner(self)
			var shape = ConvexPolygonShape2D.new()
			shape.set_points(c.get_polygon())
			shape_owner_set_transform(shape_owner, c.get_transform())
			shape_owner_add_shape(shape_owner, shape)
"

